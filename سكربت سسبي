local homeToggle2 = home:CreateToggle({
	Name = "Noclip",
	CurrentValue = false,
	Flag = "nc", 
	Callback = function(nc)
        if nc == true then
        Clip = false
        wait()
        RunService = game:GetService("RunService")
        local Noclipping = nil
        Clip = false
        wait(0.1)
        local function NoclipLoop()
            if Clip == false and plr.Character ~= nil then
                for _, child in pairs(plr.Character:GetDescendants()) do
                    if child:IsA("BasePart") and child.CanCollide == true and child.Name ~= floatName then
                        child.CanCollide = false
                    end
                end
            end
        end
        Noclipping = RunService.Stepped:Connect(NoclipLoop)
    else
        Clip = true
    end
	end,
})

local homeSlider1 = home:CreateSlider({
	Name = "walkspeed",
	Range = {0, 2},
	Increment = 0.1,
	CurrentValue = 0,
	Callback = function(sped)
        if sped > 0.6 then
            GUI:Notify({
                Title = "AY",
                Content = "I don't recommend to do above 0.5",
                duration = 2,
            })
            _G.tpwalking = false
            wait()
            _G.tpwalking = true
            while _G.tpwalking == true do
                wait()
                if plr.Character.Humanoid.MoveDirection.Magnitude > 0 then
                    plr.Character:TranslateBy(plr.Character.Humanoid.MoveDirection * tonumber(sped))
                end
            end
        else
            _G.tpwalking = false
            wait()
            _G.tpwalking = true
            while _G.tpwalking == true do
                wait()
                if plr.Character.Humanoid.MoveDirection.Magnitude > 0 then
                    plr.Character:TranslateBy(plr.Character.Humanoid.MoveDirection * tonumber(sped))
                end
            end
        end
        _G.tpwalking = false
        wait()
        _G.tpwalking = true
        while _G.tpwalking == true do
            wait()
            if plr.Character.Humanoid.MoveDirection.Magnitude > 0 then
                plr.Character:TranslateBy(plr.Character.Humanoid.MoveDirection * tonumber(sped))
            end
        end
	end,
})
