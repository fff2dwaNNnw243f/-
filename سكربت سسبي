local miscDropdown1 = misc:CreateDropdown({
	Name = "Change Team",
	Options = {"Class - D","Security Department","Mobile Task Force","Scientific Department"},
	CurrentOption = "Choose Team",
	Callback = function(team)
	if team == "Class - D" then
		game:GetService("ReplicatedStorage").Team:FireServer("CD")
	elseif team == "Security Department" then
		game:GetService("ReplicatedStorage").Team:FireServer("SD")
	elseif team == "Mobile Task Force" then
		game:GetService("ReplicatedStorage").Team:FireServer("MTF")
	elseif team == "Scientific Department" then
		game:GetService("ReplicatedStorage").Team:FireServer("ScD")
	end
	end,
})

