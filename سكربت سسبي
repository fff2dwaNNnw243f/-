_G.is = false
local homeButton4 = home:CreateButton({
	Name = "Inf Stamina",
	Callback = function()
        if _G.is == false then
            plr.Character.Stamina.MaxStamina.Value = 99999999999999999999999999999999999999999999999
            wait(0.5)
            plr.Character.Stamina.Stamina.Value = 9999999999999999999999999999999999999999999999
            while wait() do
                if plr.CharacterAdded:wait() then
                    wait(0.7)
                    plr.Character.Stamina.MaxStamina.Value = 99999999999999999999999999999999999999999999999
                    wait(0.5)
                    plr.Character.Stamina.Stamina.Value = 9999999999999999999999999999999999999999999999
                end
            end
            _G.is = true
            wait()
        else
            GUI:Notify({
                Title = "AY",
                Content = "Don't press that button 2 time's",
                Actions = { 
                    Ignore = {
                        Name = "Okay!",
                        Callback = function()
                            print("Bro why skidding")
                        end,
                    },
                },
            })
        end
	end,
})

_G.at = false
local homeToggle1 = home:CreateToggle({
	Name = "Auto Run",
	CurrentValue = false,
	Flag = "ar", 
	Callback = function(T)
        if T == true then
        _G.at = true
        while _G.at == true do
            plr.Character.Run.Running.Value = T
            wait(0.7)
        end
    elseif T == false then
        _G.at = false
    end
	end,
})

