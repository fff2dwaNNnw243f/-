_G.Bright = false
local miscButton2 = misc:CreateButton({
	Name = "full brightness",
	Callback = function()
        if _G.Bright == false then
        Lighting.Brightness = 2
        Lighting.ClockTime = 14
        Lighting.FogEnd = 100000
        Lighting.GlobalShadows = false
        Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
        _G.Bright = true
        while wait() do
            if plr.CharacterAdded:wait() then
            Lighting.Brightness = 2
            Lighting.ClockTime = 14
            Lighting.FogEnd = 100000
            Lighting.GlobalShadows = false
            Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
            wait(5)
            end
        end
        elseif _G.Bright == true then
            GUI:Notify({
                Title = "AY",
                Content = "Don't press that button 2 time's",
                Actions = { 
                    Ignore = {
                        Name = "Okay!",
                        Callback = function()
                            print("Bro why skidding")
                        end,
                    },
                },
            })
        end
	end,
})

_G.nf = false
local miscButton2 = misc:CreateButton({
	Name = "no fog",
	Callback = function()
        if _G.nf == false then
            Lighting.FogEnd = 100000
            for i,v in pairs(Lighting:GetDescendants()) do
                if v:IsA("Atmosphere") then
                    v:Destroy()
                end
            end
        _G.nf = true
        while wait() do
            if plr.CharacterAdded:wait() then
                Lighting.FogEnd = 100000
                for i,v in pairs(Lighting:GetDescendants()) do
                    if v:IsA("Atmosphere") then
                        v:Destroy()
                    end
                end
            wait(5)
            end
        end
        elseif _G.nf == true then
            GUI:Notify({
                Title = "AY",
                Content = "Don't press that button 2 time's",
