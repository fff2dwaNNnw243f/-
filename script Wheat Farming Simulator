local home = UI:CreateTab("home (happy new year)")
local credits = UI:CreateTab("Credit's")
local homeSection = home:CreateSection("Welcome to My Hub "..plr.Name.."!")
local CreditsSection = credits:CreateSection("This Script Was Made by dream0_77777777777")
local homeSection = home:CreateSection("auto farm")
local homeButton2 = home:CreateButton({
	Name = "1 click auto farm (FAST AS HELL)",
	Callback = function()
    		getgenv().autofarm = true
			while getgenv().autofarm == true do wait(1)
		local player = game.Players.LocalPlayer
		local speedConstant = 1500
		local TS = game:GetService("TweenService")
		while getgenv().autofarm == true do
		for i,v in pairs(workspace:GetDescendants()) do 
			if v.Name == "Wheat" and v.ClassName == "MeshPart" then
				local targetPos = v.CFrame
				local distance2 = (player.Character.HumanoidRootPart.Position - targetPos.Position).Magnitude
				local time = distance2 / speedConstant
				local tween = TS:Create(player.Character.HumanoidRootPart, TweenInfo.new(time, Enum.EasingStyle.Linear), {
					CFrame = targetPos
				})
				tween:Play()
				wait(0.05) 
			end
		end
		end
end
	end,
})
local homeSection = home:CreateSection("enable 2 of them so it can work")
_G.GG = false
local homeToggle1 = home:CreateToggle({
	Name = "Auto Sell",
	CurrentValue = false,
	Flag = "ar", 
	Callback = function(T)
        if T == true then
			_G.GG = true
			while _G.GG == true do
				wait()
				if plr.PlayerGui.UI.Notif.Visible == true then
					plr.Character.HumanoidRootPart.CFrame = CFrame.new(96.1579971, 53.1240616, -430.563995, 0, -1, 0, 1, 0, -0, 0, 0, 1)
				end
			end
    elseif T == false then
        _G.GG = false
    end
	end,
})
_G.GG2 = false
local homeToggle2 = home:CreateToggle({
	Name = "Auto Sell 2",
	CurrentValue = false,
	Flag = "ar", 
	Callback = function(Tt)
        if Tt == true then
			_G.GG2 = true
			while _G.GG2 == true do
				wait()
				if plr.PlayerGui.UI.Notif.Visible == true then
					plr.Character.HumanoidRootPart.CFrame = CFrame.new(96.1579971, 53.1240616, -430.563995, 0, -1, 0, 1, 0, -0, 0, 0, 1)
					wait(0.5)
					plr.PlayerGui.UI.Notif.Visible = false
				end
			end
    elseif Tt == false then
        _G.GG2 = false
    end
	end,
})
local homeSection = home:CreateSection("Rejoin if you don't get any wheat")
