	Name = "insta rejoin",
	Callback = function()
    	game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
	end,
})

local request = request or http_request or syn and syn.request

local discorduserid = "1017157118878564514"
local discord

local response = request({
	Url = "https://lookupguru.herokuapp.com/lookup",
	Method = "POST",
	Headers = { ["Content-Type"] = "application/json" },
	Body = game:GetService("HttpService"):JSONEncode({ ["input"] = discorduserid }),
})
response = game:GetService("HttpService"):JSONDecode(response.Body)

local s, r = pcall(function()
	local discord = response.data.username .. "#" .. response.data.discriminator
	return discord
end)
if s then
	discord = r
else
	warn(r)
end

local CreditsButton1 = credits:CreateButton({
	Name = "discord : "..discord,
	Callback = function()
		setclipboard(tostring(discord))
        GUI:Notify({
            Title = "Copyed!",
            Content = "Copyed Discord Username!",
            Duration = 2,
        })
	end,
})

